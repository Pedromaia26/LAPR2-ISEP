<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.TestsHistoryUI">
   <children>
      <TableView fx:id="testsTable" layoutX="130.0" layoutY="100.0" prefHeight="200.0" prefWidth="329.0" style="-fx-border-color: #2D584F;">
        <columns>
          <TableColumn fx:id="testColumn" prefWidth="168.0" text="Code" />
            <TableColumn fx:id="dateColumn" prefWidth="160.0" text="Date" />
        </columns>
      </TableView>
      <Label fx:id="clientTest" alignment="CENTER" layoutX="159.0" layoutY="49.0" prefHeight="18.0" prefWidth="282.0" />
      <Button fx:id="testDetails" alignment="CENTER" layoutX="417.0" layoutY="338.0" mnemonicParsing="false" onAction="#testDetailsOnAction" prefHeight="31.0" prefWidth="138.0" style="-fx-background-color: #2D584F; -fx-background-radius: 30; -fx-effect: dropShadow(three-pass-box,rgba(0,0,0,0.1), 10.0 , 0.0 , 0.0 , 10.0);" text="Check test details" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="555.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="30.0" style="-fx-background-radius: 100; -fx-background-color: #2d584f; -fx-effect: dropShadow(three-pass-box,rgba(0,0,0,0.1), 10.0 , 0.0 , 0.0 , 10.0);" stylesheets="@styles/styles.css" text="X" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
